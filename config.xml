<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Javi-air</name>

    <description>
        This is an example phonegap app
    </description>

    <author href="https://www.javivilchis.com" email="javivilchis@gmail.com">
        Javi-Air
    </author>
	<platform name="android" />
	<!-- Plugin -->
	<preference name="org.apache.cordova.battery-status" />
	<preference name="org.apache.cordova.spashscreen" />
	<!-- Push Plugin -->
	<plugin name="com.phonegap.plugins.pushplugin" spec="2.5.0" source="pgb" />
	<!-- icons -->
	<icon src="icon.png" />
	<icon src="/images/icons/ldpi.png" platform="android" qualifier="ldpi" />
	<icon src="/images/icons/mdpi.png" platform="android" qualifier="mdpi" />
	<icon src="/images/icons/hdpi.png" platform="android" qualifier="hdpi" />
	<icon src="/images/icons/xhdpi.png" platform="android" qualifier="xhdpi" />
	<icon src="/images/icons/xxhdpi.png" platform="android" qualifier="xxhdpi" />
	<icon src="/images/icons/xxxhdpi.png" platform="android" qualifier="xxxhdpi" />

	<!-- Splash screen -->
	<splash src="splash.png" />
	<icon src="/images/splash/ldpi-p.png" platform="android" qualifier="ldpi" />
	<icon src="/images/splash/mdpi-p.png" platform="android" qualifier="mdpi" />
	<icon src="/images/splash/hdpi-p.png" platform="android" qualifier="hdpi" />
	<icon src="/images/splash/xhdpi-p.png" platform="android" qualifier="xhdpi" />
	<icon src="/images/splash/xxhdpi-p.png" platform="android" qualifier="xxhdpi" />
	<icon src="/images/splash/xxxhdpi-p.png" platform="android" qualifier="xxxhdpi" />
	</widget>
